<main class="container">
  <h1 class="text-center">
    Productos Coder
  </h1>

  <form>
    <div class="form-group">
      <label for="nombre">Nombre</label>
      <input
        type="text"
        class="form-control"
        id="nombre"
        placeholder="Nombre"
      />
    </div>
    <div class="form-group">
      <label for="Precio">Precio</label>
      <input
        type="number"
        class="form-control"
        id="precio"
        placeholder="Precio"
      />
    </div>
    <div class="form-group">
      <label for="imagen">Url imagen</label>
      <input
        type="text"
        class="form-control"
        id="imagen"
        placeholder="imagen"
      />
    </div>

    <div>
      <button class="btn btn-success" onclick='guardarProducto()'>Cargar Producto</button>
      <a href="productos" class="btn btn-primary m-3"> Ver productos </a>
    </div>
  </form>
</main>
<script src="js/main.js"></script>